{
  "name": "effective-mobile-test-task",
  "version": "1.0.0",
  "description": "Effective Mobile test task",
  "author": "Nanakoro",
  "main": "src/index.ts",
  "scripts": {
    "typeorm:cli": "tsx --env-file=.env ./node_modules/typeorm/cli.js -d src/primary-database/configs/orm.cli.config.ts",
    "migration:generate": "npm run typeorm:cli migration:generate src/primary-database/migrations/$NAME",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert",
    "start": "node --env-file=.env dist/src/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix src --ignore-pattern 'src/primary-database/migrations/*'"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "eslint": "^9.30.1",
    "eslint-plugin-format": "^1.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
